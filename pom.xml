    <artifactId>emmenthal</artifactId>
    <version>${revision}${changelist}</version>
    <packaging>hpi</packaging>
    <name>Emmental Plugin</name>
    <description>Because Gruy√®re has no holes.</description>
    <url>https://github.com/jenkinsci/junit-plugin</url>
    <properties>
        <revision>0.1</revision>
        <changelist>-SNAPSHOT</changelist>
        <gitHubRepo>Wadeck/${project.artifactId}-plugin</gitHubRepo>
        <jenkins.version>2.263.4</jenkins.version>
        <java.level>8</java.level>
